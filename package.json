{
  "name": "betterbacks",
  "version": "0.0.0-beta.1",
  "description": "A utility to add elegant & random backgrounds to HTML elements",
  "main": "lib/index.js",
  "scripts": {
    "start-js": "webpack-dev-server",
    "build-js": "webpack --progress --colors --p",
    "build": "npm-run-all build-js",
    "tr-b": "node_modules/.bin/babel ./src/BetterBacks.js --presets @babel/preset-env --out-file lib/BetterBacks.js",
    "transpile": "npm run tr-b && node_modules/.bin/babel ./src/index.js --presets @babel/preset-env --out-file lib/index.js"
  },
  "author": "ferrousdesigner",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0-beta.6",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "keywords": [
    "betterbacks",
    "circle backgrounds",
    "square backgrounds",
    "animated background"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ferrousdesigner/betterbacks.git"
  },
  "homepage": "https://ferrousdesigner.github.io/betterbacks",
  "dependencies": {
    "stylemaker": "0.0.4-beta-3"
  }
}
